
public class Employee {

	    protected static Integer employeeCount = 0;
	    protected String id;
	    protected String name;
	    //Im pretty sure this shouldnt be static but it wont let me run unless it is
	    


// Employee Constructors

	    
	    public Employee(String name) {
	    	String s = Integer.toString(employeeCount);
	    	this.id = "1" + s;
	    	this.name = name;
	    	employeeCount++;

	    	
	    }




	  //accessor for employee count
		public int getEmployeeCount() {
			
			return employeeCount;
		}
	
		
		
		//Accessor for id
		public String getId(Integer input) {
			return id;
		}
		
		
		//mutator for id
		public void setId(String newid) {
			id = newid;
		}
		
		
		
		

		
		public void print() {
			System.out.println("Employee ID: "+ id);

		}

		public String getName() {
			return name;
		}
		

}

		
