
public class Employee {

	    private static int empIdCount = 1;
	    private int ID;
	    private String name;

// Employee Constructors
	    public Employee() {
	        name = null;
	        ID = empIdCount;
	        empIdCount++;
	    }

	    public Employee(String name, String address, int phone) {
	        this.name = name;
	        ID = empIdCount;
	        empIdCount++;
	    }
	    
// Employee Accessors
	    public String getName() {
	        return name;
	    }
	    
	    
//Employee Operations
	    //includes: Creating an order object, 
	    public

}
