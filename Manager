public class Manager extends Employee{

    
	//Manager 
    
	 public Manager(String name) {
		 	super(name);
	        employeeCount++;

	    }
	 //makes an error
	/* public Manager() {
		 	id = employeeCount;   
	        employeeCount++;

	    }
	 */

	
		public void print() {
			System.out.println("Manager: "  + name);
		}

		
		public boolean equals(Manager manager2) {
			if(name == manager2.name) {
				return true;
			}
			else {
				return false;
			}
		}
	}
