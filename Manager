
public class Manager extends Employee{

    
	//Manager 
    
	 public Manager(String name) {
		 	super(name);
		 	id = employeeCount;   
	        employeeCount++;

	    }
	 public Manager() {
		 	id = employeeCount;   
	        employeeCount++;

	    }


	
		public void print() {
			System.out.println("Manager: " + id + " " + name);
		}

		
		public boolean equals(Manager manager2) {
			if(name == manager2.name) {
				return true;
			}
			else {
				return false;
			}
		}

	 

	
	
}
